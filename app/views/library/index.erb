<div id="library">
    <h1>Library</h1>
    
  <table border="1">
    <thead>
    <tr>
      <th>Artist</th>
      <th>Album</th>
      <th>Song</th>
      <th>Year</th>
      <th>Track</th>
      <th>Rating</th>
      <th>Cover</th>
    </tr>
    </thead>
    
    <tbody>
    <tr>
      <td><%= @song.album.artist.name %></td>
      <td><%= @song.album.name %></td>
      <td><%= @song.name %></td>
      <td><%#= @song.album.year %></td>
      <td><%= @song.track_number %></td>
      <td><%= @song.rating %></td>
      <td><%= image_tag @song.album.cover.url :thumb %></td>
    </tr>
    </tbody>
  </table>
</div>