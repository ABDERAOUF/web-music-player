<div id="library">
    <h1>Library</h1>
    
  <table border="1">
    <thead>
    <tr>
      <th>Cover</th>
      <th>Artist</th>
      <th>Album</th>
      <th>Year</th>
      <th>Song</th>
      <th>Track</th>
      <th>Rating</th>
    </tr>
    </thead>
    
    <tbody>
    <% @songs_processed.each do |song| %>
    <tr>
      <td><%= image_tag song.album.cover.url(:thumb) %></td>
      <td><%= song.album.artist.name %></td>
      <td><%= song.album.name %></td>
      <td><%= song.album.year %></td>
      <td><%= song.name %></td>
      <td><%= song.track_number %></td>
      <td><%= song.rating %></td>
    </tr>
    <% end %>
    </tbody>
  </table>
</div>