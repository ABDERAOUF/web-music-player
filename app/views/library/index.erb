<div id="library">
    <h1>Library</h1>
    
  <table>
    <thead>
    <tr>
      <th>Artist</th>
      <th>Album</th>
      <th>Song</th>
      <th>Year</th>
      <th>Track</th>
      <th>Rating</th>
    </tr>
    </thead>
    
    <tbody>
    <tr>
      <td><%= @artist %></td>
      <td><%= @album %></td>
      <td><%= @title %></td>
      <td><%= @year %></td>
      <td><%= @track %></td>
      <td><%= @rating %></td>
    </tr>
    </tbody>
  </table>
</div>